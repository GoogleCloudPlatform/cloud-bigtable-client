<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
    <property>
        <name>hbase.client.connection.impl</name>
        <value>org.apache.hadoop.hbase.client.AnvilTopConnection</value>
    </property>
    <property>
        <name>google.anviltop.endpoint.host</name>
        <value>${google.anviltop.endpoint.host}</value>
    </property>
    <property>
        <name>google.anviltop.endpoint.port</name>
        <value>443</value>
    </property>
    <property>
        <name>google.anviltop.admin.endpoint.host</name>
        <value>${google.anviltop.admin.endpoint.host}</value>
    </property>
    <property>
        <name>google.anviltop.project.id</name>
        <value>testproject</value>
    </property>
    <property>
        <name>google.anviltop.auth.service.account.enable</name>
        <value>${google.anviltop.auth.service.account.enable}</value>
    </property>
    <property>
        <name>google.anviltop.auth.null.credential.enable</name>
        <value>true</value>
    </property>
    <property>
        <name>google.anviltop.call.report.directory</name>
        <value>${google.anviltop.call.report.directory}</value>
    </property>
    <property>
        <name>google.anviltop.endpoint.ip.address.override</name>
        <value>${google.anviltop.endpoint.ip.address.override}</value>
    </property>
    <property>
        <name>google.anviltop.enable.grpc.v2</name>
        <value>${google.anviltop.enable.grpc.v2}</value>
    </property>
</configuration>